let number =4,
result = total number of pairs who's HCF is 1 (example for 4 is (0,1)(1,0)(1,1)(1,2)(2,1)(1,3)(3,1)(2,3)(3,2) total pair is 9)

code--------------------------------------------
function totalPair(){
let i=2;
let arr=[]
let count = 1+(5-1)*2;
while(i<4){
if(i+1%i!==0){
count = count+2;
arr.push([i,i+1])
arr.push([i+1,i])
}
i++;
}
console.log(count)
}
